{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.10.0"
  },
  "description": "TheSHOP webpack",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverage=true",
    "start": "webpack-dev-server --mode development",
    "lint": "./node_modules/.bin/eslint ./src",
    "scss": "node-sass -w ./src/scss/main.scss ./src/css/main.min.css --output-style compressed",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "mock:api": "json-server --watch ./src/mockedDataAPI/db.json --port 4000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "author": "",
  "license": "ISC",
