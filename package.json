  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverage=true",
    "scriptName": "webpack --config etc...",
    "start": "webpack-dev-server --mode development",
    "lint": "./node_modules/.bin/eslint ./src",
    "scss": "node-sass -w ./src/scss/main.scss ./src/css/main.min.css --output-style compressed",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "mock:api": "json-server --watch ./src/mockedDataAPI/db.json --port 4000"
  },
  "husky": {
